from app.config import APP_NAME, EXCEL_FILENAME
from app.data.employee_loader import load_employee_list
from app.data.employee_details import load_employee_details
from app.data.reference_loader import load_job_options, load_costcode_options
from app.features.export_daily_time import export_daily_time

st.set_page_config(page_title=f"{APP_NAME}", layout="wide")
st.title("Timesheet Entry")

st.caption("Heartbeat: UI rendered. If this disappears after selecting an employee, tell me.")

# Date
col_date, _ = st.columns([1, 3])
with col_date:
    chosen_date = st.date_input("Date", value=date.today())

st.markdown("---")

# Job & Cost Code selectors
st.subheader("Job & Cost Code")

@st.cache_data(show_spinner=False)
def _jobs_cached():
    try:
        return load_job_options()
    except Exception:
        return []

@st.cache_data(show_spinner=False)
def _codes_cached():
    try:
        return load_costcode_options()
    except Exception:
        return []

jobs = _jobs_cached()
codes = _codes_cached()

c1, c2 = st.columns(2)
with c1:
    job_choice = job_options = (locals().get("job_display")
               or locals().get("job_options")
               or locals().get("jobs_display")
               or [])
job_options = (locals().get("job_display") or locals().get("job_options") or locals().get("jobs_display") or [])
# Build job list using tolerant loader
from pathlib import Path
from utils_jobs import load_jobs_auto, build_job_options

xlsx_path = (Path(__file__).resolve().parent.parent / "Timesheet Apps.xlsx")

jobs_df = None
job_options = []
job_warning = None

try:
    jobs_df = load_jobs_auto(xlsx_path)
    job_options = build_job_options(jobs_df)
    if not job_options:
        job_warning = "No jobs found after cleaning."
except FileNotFoundError:
    job_warning = f"Workbook not found at: {xlsx_path}"
except ValueError as e:
    job_warning = str(e)
except Exception as e:
    job_warning = f"Unexpected error reading jobs: {e}"

if job_warning:
    st.warning(f"Jobs list not loaded: {job_warning}")
    job_choice = st.selectbox("Job (no jobs found)", options=[], index=None, disabled=True)
else:
# --- jobs auto-fill (non-invasive) ---
try:
    from pathlib import Path as _P
    from utils_jobs import load_jobs_auto as _load, build_job_options as _bjo
    _xlsx = (_P(__file__).resolve().parent.parent / "Timesheet Apps.xlsx")
    _df_jobs = _load(_xlsx)
    if not ('job_options' in locals() and job_options):
        job_options = _bjo(_df_jobs)
except Exception as _e:
    try:
        import streamlit as st
        st.caption(f"Jobs auto-loader: {_e}")
    except:
        pass
# --- end jobs auto-fill ---
    job_choice = st.selectbox("Job", options=job_options, index=None, placeholder="Select a job…", key="job_select_main")
if not _jobs:
    st.warning(f"Jobs list not loaded: {_jobs_msg}")
job_options = [f"{j['Job Number']} - {j['Area Number']} - {j['Description']}" for j in _jobs]
if not job_options:
    job_options = []
st.selectbox("Job Number - Area Number - Description", job_options, key="job_choice")


