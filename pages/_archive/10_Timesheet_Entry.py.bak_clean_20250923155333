import streamlit as st

# ---- tolerant jobs loader (inserted) ----
import re
from pathlib import Path

def _norm(s):
    return re.sub(r"\s+", " ", str(s).strip()).lower()

def _pick(cols, candidates):
    norm = { _norm(c): c for c in cols }
    for wanted in candidates:
        k = _norm(wanted)
        if k in norm:
            return norm[k]
    # fuzzy: startswith
    for k, orig in norm.items():
        for wanted in candidates:
            if k.startswith(_norm(wanted)):
                return orig
    return None

    """
    Returns (jobs_list, message)
    jobs_list = [{"Job Number","Area Number","Description"}, ...]
    """
    try:
        import pandas as pd
        xlsx = Path(xlsx_path)
        xl = pd.ExcelFile(xlsx)
        for sheet in xl.sheet_names:
            try:
                df = xl.parse(sheet)
                if df.empty:
                    continue
                cols = list(df.columns)
                job_col  = _pick(cols, ["Job Number","Job No","Job","#","Work Order","WO"])
                area_col = _pick(cols, ["Area Number","Area No","Area"])
                desc_col = _pick(cols, ["Description","Job Description","Desc"])
                if job_col and area_col and desc_col:
                    out = []
                    for _, r in df.iterrows():
                        j = str(r.get(job_col,"")).strip()
                        a = str(r.get(area_col,"")).strip()
                        d = str(r.get(desc_col,"")).strip()
                        if j or a or d:
                            out.append({"Job Number": j, "Area Number": a, "Description": d})
                    if out:
                        return out, f"Loaded {len(out)} jobs from sheet '{sheet}'."
            except Exception:
                continue
        return [], "no suitable columns found on any sheet (openpyxl)"
    except Exception as e:
        return [], f"error reading jobs: {e}"
# ---- end tolerant loader ----
# ===== Autoload job & cost options (robust) - injected =====
from pathlib import Path as __P
import pandas as __pd
import streamlit as __st

# ensure variables exist even if load fails
job_options  = [] if "job_options"  not in locals() else job_options
cost_options = [] if "cost_options" not in locals() else cost_options

def __norm(s):
    return str(s or "").strip().lower().replace("Ã¢â‚¬â€œ","-").replace("Ã¢â‚¬â€","-")

def __norm_cols(df):
    return {__norm(c): c for c in df.columns}

def __has_all(cols, req_parts):
    # "job number" -> any header that includes both "job" and "number"
    for rp in req_parts:
        parts = rp.split()
        found = False
        for k in cols.keys():
            if all(p in k for p in parts):
                found = True; break
        if not found:
            return False
    return True

def __find_composite_col(cols):
    # header contains job + area + desc somewhere in it
    for k, orig in cols.items():
        if "job" in k and "area" in k and ("desc" in k or "description" in k):
            return orig
    return None

def __collect_jobs_from_df(df):
    cols = __norm_cols(df)
    out  = []

    # case A: three separate columns (job number / area number / description)
    if __has_all(cols, ["job number", "area number", "description"]):
        # pick the best matching originals
        J = next(v for k,v in cols.items() if "job" in k and "number" in k)
        A = next(v for k,v in cols.items() if "area" in k and "number" in k)
        D = next(v for k,v in cols.items() if ("desc" in k or "description" in k))
        tmp = df[[J, A, D]].astype(str).fillna("")
        out = (tmp[J].str.strip() + " - " + tmp[A].str.strip() + " - " + tmp[D].str.strip()).tolist()
        out = [s for s in out if s.strip()]
        return out

    # case B: single composite column like Ã¢â‚¬Å“Job Number Ã¢â‚¬â€œ Area Number Ã¢â‚¬â€œ DescriptionÃ¢â‚¬Â
    composite = __find_composite_col(cols)
    if composite:
        ser = df[composite].astype(str).fillna("").map(str).str.strip()
        out = [s for s in ser.tolist() if s]
        return out

    return []

def __read_jobs_pandas(xlsx):
    try:
        xl = __pd.ExcelFile(xlsx)
        all_opts = []
        for s in xl.sheet_names:
            try:
                df = xl.parse(s)
                opts = __collect_jobs_from_df(df)
                if opts:
                    all_opts.extend(opts)
            except Exception:
                pass
        # de-dup while preserving order
        seen = set(); uniq=[]
        for v in all_opts:
            if v not in seen:
                seen.add(v); uniq.append(v)
        return uniq, None if uniq else "no suitable columns found on any sheet"
    except Exception as e:
        return [], f"[jobs pandas] {e}"

def __read_jobs_openpyxl(xlsx):
    try:
        import openpyxl
        wb = openpyxl.load_workbook(filename=str(xlsx), read_only=True, data_only=True)
        all_opts = []
        for ws in wb.worksheets:
            rows = list(ws.iter_rows(values_only=True))
            if not rows: continue
            hdr = [str(h or "").strip() for h in rows[0]]
            df  = __pd.DataFrame(rows[1:], columns=hdr)  # reuse the same parser
            opts = __collect_jobs_from_df(df)
            if opts:
                all_opts.extend(opts)
        seen=set(); uniq=[]
        for v in all_opts:
            if v not in seen:
                seen.add(v); uniq.append(v)
        return uniq, None if uniq else "no suitable columns found on any sheet (openpyxl)"
    except Exception as e:
        return [], f"[jobs openpyxl] {e}"

try:
    __root = __P(__file__).resolve().parents[1]
    __xlsx = __root / "Timesheet Apps.xlsx"

    if not job_options:
        # try pandas across ALL sheets
        job_options, __jerr = __read_jobs_pandas(__xlsx)
        if not job_options:
            # fallback to openpyxl across ALL sheets
            job_options, __jerr2 = __read_jobs_openpyxl(__xlsx)
            if not job_options:
                __st.warning(f"Jobs list not loaded: {__jerr2 or __jerr}")

    # cost loader left as-is (we already loaded 240)
    if not cost_options:
        # Very small, tolerant cost loader (pandas across all sheets)
        try:
            xl = __pd.ExcelFile(__xlsx)
            opts = []
            for s in xl.sheet_names:
                df = xl.parse(s)
                cols = __norm_cols(df)
                if __has_all(cols, ["cost code", "description"]):
                    C = next(v for k,v in cols.items() if "cost" in k and "code" in k)
                    D = next(v for k,v in cols.items() if "desc" in k or "description" in k)
                    t = df[[C, D]].astype(str).fillna("")
                    opts.extend((t[C].str.strip() + " - " + t[D].str.strip()).tolist())
            seen=set(); uniq=[]
            for v in opts:
                if v and v not in seen:
                    seen.add(v); uniq.append(v)
            cost_options = uniq
        except Exception as e:
            __st.warning(f"Cost codes not loaded: {e}")

    __st.caption(f"Loaded {len(job_options)} jobs, {len(cost_options)} cost codes.")
except Exception as __e:
    __st.warning(f"Job/Cost loader error: {__e}")
# ===== End injected loader =====
# ---- Fallback options loader (added) ----
# If job_options / cost_options were not created earlier, try to build them
# from "Timesheet Apps.xlsx" using common sheet/column names.
try:
    import pandas as _pd
    from pathlib import Path as _Path

    def _load_xlsx_df(_xlsx, candidates, needed_cols):
        try:
            xl = _pd.ExcelFile(_xlsx)
        except Exception:
            return None
        for nm in candidates:
            if nm in xl.sheet_names:
                try:
                    df = xl.parse(nm)
                    # normalize column names for matching
                    cols_norm = {c.strip(): c for c in df.columns if isinstance(c, str)}
                    # do we have a superset of the columns we need?
                    if all(any(k.lower() == want.lower() for k in cols_norm) for want in needed_cols):
                        return df
                except Exception:
                    continue
        return None

    # workbook next to the project root file locations
    _root = _Path(__file__).resolve().parents[1]
    _xlsx = _root / "Timesheet Apps.xlsx"

    # Build jobs if missing or empty
    if "job_options" not in locals() or not job_options:
        _jobs_df = _load_xlsx_df(
            _xlsx,
            ["Jobs", "Job List", "Jobs List", "JobsList"],
            ["Job Number", "Area Number", "Description"],
        )
        job_options = []
        if _jobs_df is not None and not _jobs_df.empty:
            # Be tolerant to header variants by matching case-insensitively
            def _col(df, name):
                for c in df.columns:
                    if isinstance(c, str) and c.strip().lower() == name.lower():
                        return c
                return name  # best-effort
            jn = _col(_jobs_df, "Job Number")
            an = _col(_jobs_df, "Area Number")
            ds = _col(_jobs_df, "Description")
            # stringify and build display text
            _tmp = _jobs_df[[jn, an, ds]].astype(str).fillna("")
            job_options = (_tmp[jn] + " - " + _tmp[an] + " - " + _tmp[ds]).tolist()

    # Build cost codes if missing or empty
    if "cost_options" not in locals() or not cost_options:
        _cost_df = _load_xlsx_df(
            _xlsx,
            ["Cost Codes", "Cost Code List", "Codes", "CostCodes"],
            ["Cost code", "Description"],
        )
        cost_options = []
        if _cost_df is not None and not _cost_df.empty:
            def _col(df, name):
                for c in df.columns:
                    if isinstance(c, str) and c.strip().lower() == name.lower():
                        return c
                return name
            cc = _col(_cost_df, "Cost code")
            ds = _col(_cost_df, "Description")
            _tmp = _cost_df[[cc, ds]].astype(str).fillna("")
            cost_options = (_tmp[cc] + " - " + _tmp[ds]).tolist()
except Exception:
    # DonÃƒÆ’Ã‚Â¢ÃƒÂ¢Ã¢â‚¬Å¡Ã‚Â¬ÃƒÂ¢Ã¢â‚¬Å¾Ã‚Â¢t crash the page if workbook/sheets arenÃƒÆ’Ã‚Â¢ÃƒÂ¢Ã¢â‚¬Å¡Ã‚Â¬ÃƒÂ¢Ã¢â‚¬Å¾Ã‚Â¢t available
    pass
# ---- End fallback options loader ----

